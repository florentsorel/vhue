{
  "name": "vhue",
  "version": "0.2.0",
  "description": "Philips Hue component for Vue.js",
  "author": {
    "name": "Rtransat",
    "email": "rtransat@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtransat/vhue.git"
  },
  "homepage": "https://github.com/rtransat/vhue",
  "keywords": [
    "vhue",
    "vue",
    "vuejs",
    "hue",
    "philips hue"
  ],  
  "main": "dist/vhue.ssr.js",
  "module": "dist/vhue.esm.js",
  "unpkg": "dist/vhue.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "README.md",
    "LICENSE",
    "!src/serve-dev.*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint": "./node_modules/.bin/eslint src/"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "color": "^3.1.2",
    "cross-env": "^6.0.3",
    "dart-sass": "^1.24.2",
    "minimist": "^1.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^2.1.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "5.1.1",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
